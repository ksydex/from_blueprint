@page
@model from_blueprint.Pages_Admin_House.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>House</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.House[0].Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.House[0].Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.House[0].Area)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.House[0].Floors)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.House[0].Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.House[0].Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.House[0].Image)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.House[0].CreatedDate)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.House)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Area)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Floors)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Image)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedDate)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
    }
    </tbody>
</table>